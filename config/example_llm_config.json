{
    "model_settings": {
        "model_path": "models/default_model.gguf",
        "context_length": 2048,
        "temperature": 0.7,
        "top_p": 0.95,
        "repeat_penalty": 1.1
    },
    "resource_limits": {
        "max_memory_mb": 2048,
        "max_cpu_threads": 4,
        "batch_size": 512
    },
    "safety_settings": {
        "command_review_required": true,
        "unsafe_command_patterns": [
            "rm -rf",
            "sudo",
            "chmod 777"
        ],
        "sanitize_patterns": [
            "password=.*",
            "token=.*",
            "key=.*"
        ]
    },
    "session_settings": {
        "auto_summarize": true,
        "summary_interval_minutes": 30,
        "max_history_commands": 1000,
        "log_level": "INFO"
    }
} 